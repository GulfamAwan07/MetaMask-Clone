{
  "manifest_version": 3,
  "name": "MetaMask-G",
  "icons": {
    "16": "icons/icon2.png",
    "32": "icons/icon2.png",
    "64": "icons/icon2.png",
    "128": "icons/icon2.png"
  },
  "version": "1.0.0",
  "permissions" : ["storage"],

  "background": {
    "service_worker": "background.js",
    "type": "module"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "64": "icons/icon2.png",
      "128": "icons/icon2.png"
    }
  }
}
